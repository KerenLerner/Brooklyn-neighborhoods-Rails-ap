<p id="notice"><%= notice %></p>

<h1>Listing Seekers</h1>

<table>
  <thead>
    <tr>
      <th>Seeker name</th>
      <th>Seeker rent</th>
      <th>Seeker park</th>
      <th>Seeker activities</th>
      <th>Seeker architecture</th>
      <th>Seeker commute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seekers.each do |seeker| %>
      <tr>
        <td><%= seeker.seeker_name %></td>
        <td><%= seeker.seeker_rent %></td>
        <td><%= seeker.seeker_park %></td>
        <td><%= seeker.seeker_activities %></td>
        <td><%= seeker.seeker_architecture %></td>
        <td><%= seeker.seeker_commute %></td>
        <td><%= link_to 'Show', seeker %></td>
        <td><%= link_to 'Edit', edit_seeker_path(seeker) %></td>
        <td><%= link_to 'Destroy', seeker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seeker', new_seeker_path %>
